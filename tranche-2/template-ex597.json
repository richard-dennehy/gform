{
  "_id": "EX597-first",
  "formName": "Alcoholic Ingredients Relief",
  "description": "",
  "dmsSubmission": {
    "customerId": "????nino",
    "classificationType": "????BT-NRU-Environmental",
    "businessArea": "????FinanceOpsCorpT"
  },
  "authConfig":{
    "authModule": "legacyEEITTAuth",
    "regimeId": "GF"
  },
  "submitSuccessUrl": "http://www.google.co.uk",
  "submitErrorUrl": "http://www.yahoo.co.uk",
  "sections": [
    {
      "title": "Before you start",
      "fields": [
        {
          "id": "beforeYouStartInfo1",
          "type": "info",
          "label": "",
          "infoText": "To use this service, make sure you have the following information:  \n* Your Unique Taxpayer Reference (UTR) number\n* Business address and phone number\n* Address where alcohol is used\n* Start and end dates of the claim\n* Details of where you want any payment to go\n* Details of final product made (including final product weight in kilograms)\n* Quantity of alcohol used and amount of duty due  <br><br>\n\nYou'll also need to upload copies of the following documents:  \n* Original invoice from the supplier\n* Proof that your product is eligible (contains no more than 5 litres of alcohol)\n* Copies of recipes or production sheets\n* Complete worksheet showing how you arrived at your claim figure"
        }
      ]
    },
    {
      "title": "Which of the following best describes your business activity?",
      "fields": [
        {
          "id": "businessActivityChoice",
          "type": "choice",
          "label": "",
          "choices": [
            "I am the manufacturer of an eligible article",
            "I have supplied duty paid alcohol used in the production of an eligible article"
          ],
          "errorMessage": "Please choose the option that best describes your business activity"
        }
      ]
    },
    {
      "title": "Enter your business details",
      "fields": [
        {
          "id": "businessName",
          "type": "text",
          "label": "Business name"
        },
        {
          "id": "businessAddress",
          "type": "address",
          "label": "Business address",
          "international": "true"
        },
        {
          "id": "phoneNum",
          "type": "text",
          "format": "telephoneNumber",
          "label": "Phone number"
        }
      ]
    },
    {
      "title": "Tell us where alcohol is used",
      "fields": [
        {
          "id": "hasAlcoholAddressChangedChoice",
          "type": "choice",
          "label": "Has the address where alcohol is used changed since your last claim?",
          "format": "yesno",
          "errorMessage": "Select yes if the address where alcohol is used is the same as the business address"
        }
      ]
    },
    {
      "title": "Enter the address where alcohol is used",
      "includeIf": "${hasAlcoholAddressChangedChoice = 0}",
      "fields": [
        {
          "id": "alcoholAddress",
          "type": "address",
          "label": "",
          "helpText": "Enter the address lines and postcode of the address where alcohol is used",
          "international": "true"
        }
      ]
    },
    {
      "title": "Enter your contact person",
      "description" : "This is the person we will contact if we need to discuss your claim.",
      "fields": [
        {
          "id": "contactFullName",
          "type": "text",
          "label": "Full name",
          "errorMessage": "Enter your contact person's full name"
        },
        {
          "id": "contactEmail",
          "type": "text",
          "label": "Email address (optional)",
          "format": "email",
          "mandatory": "false",
          "errorMessage": "Enter your contact person's full name"
        }
      ]
    },
    {
      "title": "Does your claim cover a production or manufacture period of three months?",
      "fields": [
        {
          "id": "claimCovers3MonthsChoice",
          "type": "choice",
          "label": "",
          "format": "yesno"
        }
      ]
    },
    {
      "title": "Is your production or manufacture period less than three months because you have stopped trading?",
      "includeIf": "${claimCovers3MonthsChoice = 1}",
      "fields": [
        {
          "id": "stoppedTradingChoice",
          "type": "choice",
          "label": "",
          "format": "horizontal",
          "choices": [
            "Yes",
            "No"
          ],
          "optionHelpText": [
            "",
            "Claims must cover a 3 month period. If this is not the case, you can continue with this claim but we will contact you to discuss reasons before considering your claim.  \nRefer to [Excise Notice 41, section 5.4](https://www.gov.uk/government/publications/excise-notice-41-alcoholic-ingredients-relief/excise-notice-41-alcoholic-ingredients-relief#para54)"
          ]
        }
      ]
    },
    {
      "title": "Enter the start and end dates for this claim",
      "shortName": "Your claim period",
      "fields": [
        {
          "id": "claimStartDate",
          "type": "date",
          "label": "Start date",
          "helpText": "Example: 31 09 2017",
          "errorMessage": "Please enter a valid start date"
        },
        {
          "id": "claimEndDate",
          "type": "date",
          "label": "End date",
          "helpText": "Example: 31 12 2017",
          "format": "after ${claimStartDate}",
          "errorMessage": "Please enter a valid end date.  Note this must be after the start date."
        }
      ]
    },
    {
      "title": "Tell us about the products made or prepared using the alcoholic ingredients you are claiming relief for",
      "fields": [
        {
          "id": "enterOnScreenChoice",
          "type": "choice",
          "label": "",
          "choices": [
            "I will enter product details on screen",
            "I will upload a file containing my product details"
          ],
          "optionHelpText": [
            "",
            "You can upload your product list at the end of this claim"
          ]
        }
      ]
    },
    {
      "title": "Enter details of the beverages and articles made or prepared using the alcoholic ingredients",
      "description": "Enter the description and quantity of beverages and articles below.",
      "includeIf": "${enterOnScreenChoice = 0}",
      "fields": [
        {
          "id": "productGroup",
          "type": "group",
          "label": "",
          "format": "horizontal",
          "repeatsMax": 100,
          "fields": [
            {
              "id": "productDescription",
              "type": "text",
              "label": "Product description",
              "errorMessage": "Please enter a product description"
            },
            {
              "id": "quantityInKg",
              "type": "text",
              "label": "Quantity in kg",
              "format": "number(12,2,'kg')",
              "errorMessage": "Please enter a quantity"
            }
          ]
        },
        {
          "id": "!!!TODO:presentationHintTotal_alcoholQuantityTotal",
          "type": "text",
          "label": "Total",
          "format": "number(12,2,'kg')"
        }
      ]
    },
    {
      "title": "Tell us the amount of alcohol ingredients used and duty paid",
      "description": "Select the alcoholic ingredients used in your product below.",
      "fields": [
        {
          "id": "spiritsGroup",
          "type": "group",
          "label": "Spirit",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "spiritsQuantity",
              "type": "text",
              "format": "number(11, 2, 'litres')",
              "label": "Quantity in litres"

            },
            {
              "id": "spiritsDutyDue",
              "type": "text",
              "format": "sterling",
              "label": "Amount of duty due"
            }
          ]
        },
        {
          "id": "beerGroup",
          "type": "group",
          "label": "Beer",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "beerQuantity",
              "type": "text",
              "format": "number(11, 2, 'litres')",
              "label": "Quantity in litres"
            },
            {
              "id": "beerDutyDue",
              "type": "text",
              "format": "sterling",
              "label": "Amount of duty due"
            }
          ]
        },
        {
          "id": "wineGroup",
          "type": "group",
          "label": "Wine",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "wineQuantity",
              "type": "text",
              "format": "number(11, 2, 'litres')",
              "label": "Quantity in litres"
            },
            {
              "id": "wineDutyDue",
              "type": "text",
              "format": "sterling",
              "label": "Amount of duty due"
            }
          ]
        },
        {
          "id": "ciderGroup",
          "type": "group",
          "label": "Cider",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "ciderQuantity",
              "type": "text",
              "format": "number(11, 2, 'litres')",
              "label": "Quantity in litres"
            },
            {
              "id": "ciderDutyDue",
              "type": "text",
              "format": "sterling",
              "label": "Amount of duty due"
            }
          ]
        },
        {
          "id": "!!!TODO_presentationHint_total_totalDutyPaid",
          "type": "text",
          "label": "Total amount of duty paid",
          "format": "sterling",
          "shortName": "Total amount of duty for this claim period"
        }
      ]
    },
    {
      "title": "Do you have any underclaims or overclaims from previous periods to declare?",
      "fields": [
        {
          "id": "overUnderClaimsYesNo",
          "type": "choice",
          "label": "",
          "format": "yesno"
        }
      ]
    },
    {
      "title": "Are there underclaims or overclaims?",
      "includeIf": "${overUnderClaimsYesNo = 0}",
      "fields": [
        {
          "id": "overUnderClaimsChoice",
          "type": "choice",
          "label": "",
          "choices": [
            "Over claim",
            "Under claim",
            "Both"
          ]
        }
      ]
    },
    {
      "title": "Enter the start and end dates for this underclaim",
      "fields": [
        {
          "id": "!!!TODO_includeIfwithOR_underclaimStartDate",
          "type": "date",
          "label": "Start date",
          "helpText": "Example: 31 09 2017",
          "errorMessage": "Please enter a valid start date"
        },
        {
          "id": "overclaimEndDate",
          "type": "date",
          "label": "End date",
          "helpText": "Example: 31 12 2017",
          "format": "after ${underclaimStartDate}",
          "errorMessage": "Please enter a valid end date.  Note this must be after the start date."
        }
      ]
    },
    {
      "title": "Enter the details for underclaims from a previous period",
      "description": "Select the alcoholic ingredients used in your product below.",
      "fields": [
        {
          "id": "spiritsUnderclaimGroup",
          "type": "group",
          "label": "Spirit",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "amountUnderclaimedSpirits",
              "type": "text",
              "format": "sterling",
              "label": "Amount of duty underclaimed"
            }
          ]
        },
        {
          "id": "beerUnderclaimGroup",
          "type": "group",
          "label": "Beer",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "amountUnderclaimedBeer",
              "type": "text",
              "format": "sterling",
              "label": "Amount of duty underclaimed"
            }
          ]
        },
        {
          "id": "wineUnderclaimGroup",
          "type": "group",
          "label": "Wine/Made-wine including fortified wine",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "amountUnderclaimedWine",
              "type": "text",
              "format": "sterling",
              "label": "Amount of duty underclaimed"
            }
          ]
        },
        {
          "id": "ciderUnderclaimGroup",
          "type": "group",
          "label": "Cider",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "amountUnderclaimedCider",
              "type": "text",
              "format": "sterling",
              "label": "Amount of duty underclaimed"
            }
          ]
        },
        {
          "id": "!!!TODO_presentationHint_total_underclaimsTotal",
          "type": "text",
          "label": "Under claims total",
          "format": "sterling"
        }
      ]
    }
  ]
}
